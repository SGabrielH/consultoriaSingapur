{
  "name": "electron-sidebar",
  "version": "1.0.0",
  "description": "Una aplicaci√≥n de Electron con una Side Bar interactiva",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "author": "Tu Nombre",
  "license": "MIT",
  "dependencies": {
    "electron": "^34.0.1"
  },
  "devDependencies": {
    "electron-builder": "^23.0.0"
  },
  "build": {
    "appId": "com.tunombre.electron-sidebar",
    "productName": "Electron Side Bar",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.html",
      "login.html",
      "styles.css",
      "script.js"
    ],
    "win": {
      "target": "nsis",
      "icon": "icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.png"
    }
  }
}